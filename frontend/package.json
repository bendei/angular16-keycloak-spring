{
  "name": "frontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-hmr": "ng serve --hmr",
    "build": "ng build --prod",
    "test": "ng test --progress=false --code-coverage",
    "test-ci": "npm run test -- --watch=false",
    "ts-lint": "ng lint",
    "compress": "node scripts/compress.js",
    "lint": "concurrently -r \"npm run ts-lint -- --fix\" \"npm run stylelint\"",
    "lint-ci": "concurrently -r \"npm run ts-lint\" \"npm run stylelint\"",
    "stylelint": "stylelint --syntax scss \"src/**/*.scss\"",
    "commit-check": "npm run test && ng lint --fix && npm run stylelint",
    "e2e": "ng e2e",
    "generate-api": "openapi-generator-cli generate -i ../openAPI.yaml -g typescript-angular -o src/app/openapi-generated-sources  -p=removeOperationIdPrefix=true",
    "mock-server": "json-server --watch mockdata.json --port 3000",
    "start-dev": "ng serve --configuration=dev --proxy-config proxy.conf.json",
    "start-prod": "ng serve --configuration prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@popperjs/core": "^2.11.6",
    "@swimlane/ngx-datatable": "^20.1.0",
    "@types/lodash": "^4.14.197",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "font-awesome": "^4.7.0",
    "keycloak-angular": "^15.0.0",
    "keycloak-js": "^22.0.5",
    "lodash": "^4.17.21",
    "ngx-logger": "^5.0.12",
    "ngx-toastr": "^17.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "~17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@angular/language-service": "17.1.1",
    "@angular/localize": "^17.1.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "openapi-typescript": "^6.7.4",
    "typescript": "~5.2.0"
  }
}
